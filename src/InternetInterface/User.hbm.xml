<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="InternetInterface.Models"  assembly="InternetInterface">
  <class name="User" table="InetUsers" schema="accessright">
    <id name="ID" type="Int32"  column="ID">
        <generator class="increment"/>
      </id>
    <property name="NAME" />
    <property name="EMAIL" />
    <property name="TELNUM" />
    <property name="ADRESS" />
    <property name="PASS" />
    <property name="REGDATE" type="Date" />
  </class>
</hibernate-mapping>
