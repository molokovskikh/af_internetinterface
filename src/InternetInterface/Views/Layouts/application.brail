<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<head>
	<title>Интернет интерфейс</title>

	${app.JS("jquery-1.6.1.js",
	"jquery.validate.js",
	"Common.js",
	"jquery.maskedinput-1.2.2.js",
	"jquery.reveal.js",
	"jquery.nyroModal.custom.js",
	"jquery-ui-1.8.16.custom.js",
	"jquery.ui.datepicker-ru.js",
	"jquery.tablesorter.js")}

	${app.CSS("Style.css",
		"base.css",
		"datePicker.css",
		"tablesorter.css",
		"reveal.css",
		"filter.css",
		"jquery-ui-1.8.16.custom.css")}
</head>
<body>
<div class="non-printable"> 
	<% OutputSubView("""\Subview\MapHead""") %>
</div> 
	<% if IsDefined("Message"): %>
	<div class="flash">
		<% if Message isa string: %>
		<div class="notice">
			${Message}
		</div>
		<% else: %>
		<div class="${Message.GetClass()}">
			${Message}
		</div>
		<% end %>
	</div>
	<% end %>
${ChildOutput}
</body>
</html>