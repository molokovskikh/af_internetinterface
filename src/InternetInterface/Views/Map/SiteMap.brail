<link href="../Styles/style.css" type="text/css" rel="stylesheet" />
<link rel="stylesheet" href="../Styles/base.css" type="text/css" media="screen" />

<div id="main" style="width: 50%;">
<div id="container">
<div class="content">
<table class="table"  >
<thead>
<tr valign="top">
   <th > Интерфейс для партнера: ${PARTNERNAME}</th>
</tr>
 </thead>   
 <tbody>
     <%if PartnerAccessSet.AccesPartner("RP"): %>
    <tr>
        <td>
        <a href = "../Register/RegisterPartner.rails" >Регистрация партнера</a>
         </td>
    </tr>
    <%end %>
    <%if PartnerAccessSet.AccesPartner("RC"): %>
    <tr>
        <td>
        <a href = "../Register/RegisterClient.rails" >Регистрация нового абонента</a>
         </td>
    </tr>
    <%end %>
    <%if PartnerAccessSet.AccesPartner("GCI"): %>
    <tr>
        <td>
        <a href = "../Search/SearchUsers.rails" >Найти абонента</a>
        </td>
    </tr>
    <%end %>
    <%if PartnerAccessSet.AccesPartner("CD"): %>
    <tr>
        <td>
        <a href = "../Search/SearchBy.rails?CloseDemand=true" > Закрыть заявки</a>
        </td>
    </tr>
    <%end %>
    <%for i, item in enumerate(AccessList):  %>
        <tr>              
    Доступ к опции ${item} разрешен <br />
   </tr>
   <% end %>
   <tr>
   <td>
      <form class="form" id="Exit" name="Exit" method="post" action="${siteroot}/ExitPartner/Yes" >
      <button type="submit" class="button">
      <img alt="Save" src="../Images/application_edit.png"> Выход
      </button>
      </form>
    </td>
  </tr>
    </tbody>
</table>
</div>
</div>
</div>  
