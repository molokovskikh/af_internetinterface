<% OutputSubView("""\Subview\MapHead""") %>
<body>
    <script type="text/javascript">
        Date.firstDayOfWeek = 0;
        Date.format = 'dd.mm.yyyy';
        $(function () {
            $('.hasDatepicker').datePicker({ startDate: '01/01/1996' });
        });
    </script>

<center>
    <div class="filterDiv">
        <form method="get" action="${siteroot}/Payers/ShowAgent">
            <p>Выберете регистратора по которому будет применен фильтр</p>
            <select name="agent" id="agent" style="width: 70%" >
            <%for i, agent in enumerate(agents):%>
            <option id="agent${agent.Id}" value="${agent.Id}" ${"selected" if (agent.Id == agentId)}> ${agent.Name} </option>
            <% end %>
            </select>
            <br />
            <input type="text" name="startDate" id="datepicker" class="hasDatepicker">
            <br />
            <input type="text" name="endDate" id="Text1" class="hasDatepicker">
            <br />
            <input id="SearchButton" type="submit" value="Показать"  align="right" />
        </form>
    </div>
</center>
</body>