		<style type="text/css">
		    
		.turn-me-into-datepicker {
			float: left;
			margin:10px;
			font-size:9px;
		}				.jCalendar		{			font-size:10px;			}
		.hasDatepicker
		{
			margin-top:10px;
			float:none;
			}
			
		.filterDiv
		{
			width:950px;
			height:300px;
			background-color:White;
			text-align:left;
			border: 1px solid black;
			 padding: 10px;
			 margin:10px;
		}
		
		.table .whiteBr
		{
			background-color:White;
			color:Black;
			border: 1px solid black;
			}
			
		.monthDiv
		{
		    width:80px;
		    height:20px;
		    background-color:#D5E6D0;
		    border: 1px solid #5f6060;
		    float:left;
		    text-align:center;
		    }
	</style>
	<script type="text/javascript">
        Date.format = 'dd.mm.yyyy';
        $(function () {
            $('.graph_date').datepicker();
            //$('.turn-me-into-datepicker').datepicker();
            //$('#endDate').val('${System.DateTime.Now.ToShortDateString()}');
            $('.monthDiv').css('background-color', '#D5E6D0');
            $('#${colorId}').parent().css('background-color', '#7ec469');
            $('.monthDiv').click(function () {
                var month = $(this).children('a:first').attr('id');
                var dayCount = 32 - new Date($('#year').val(), Number(month - 1), 32).getDate();
                var formatedStartDate = "01." + month + "." + $('#year').val();
                var formatedEndDate = dayCount + "." + month + "." + $('#year').val();
                $('#startDate').val(formatedStartDate);
                $('#endDate').val(formatedEndDate);
                $('#showBut').click();
            });
        });
	</script>

<center>
	<div class="filterDiv">
