<table style="border:1px solid #AAA" class="table">
	<thead>
		<tr>
			<th colspan=3>${message}</th>
		</tr>
		<tr>
			<th>Дата действия</th>
			<th>Сумма</th>
			<th>Действие</th>
		</tr>
	</thead>
	<tbody>
	<%for i, payment in enumerate(payments): %>
		<tr>
			<td>${payment.RegistrationDate.ToShortDateString()}</td>
			<td>${payment.Sum}</td>
			<td>${payment.Comment}</td>
		</tr>
	<%end %>
	</tbody>
</table>