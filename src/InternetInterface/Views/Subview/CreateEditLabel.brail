<tr>
    <td><input type="text" id="LabelName"  name="LabelName"  value="${LabelName}" style="width: 80%; " /></td>
</tr>
<tr>
    <td>
            <select name="labelcolor" id="labelcolor" style="width: 100%" >
            <% for i, color in enumerate(labelColors): %>
            <option id="color${i}" value=${color} style="background-color: ${color}"> ${color} </option>
            <% end %>
    </td>
</tr>
