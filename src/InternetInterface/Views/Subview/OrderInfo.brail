			<% else: %>
			<% if OrderInfo.Order.EndPoint != null: %>
				<a href="#" class="big-link" data-reveal-id="deletePointDialog">
					<input type="hidden" value="${OrderInfo.ClientConnectInfo.endpointId}" />
