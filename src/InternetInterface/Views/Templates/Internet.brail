<div class="registration-sub-block">
	<h3 class="form-like">!{internet.Service.HumanName}</h3>
	<div>
		${app.Edit("internet.ActivatedByUser", {"data-bind": "checked: internetActivated"})}
		${app.GetValidationError("client.ActivatedByUser")}
	</div>
	<div>
		${app.Label("client.Tariff")}
		${app.Edit("client.Tariff", {@emptyValue: "", "data-bind": "value: tariff"})}
		${app.GetValidationError("client.Tariff")}
	</div>
</div>